    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,
    "baseUrl": "./src",
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "nodenext",
    "lib": [
      "esnext"
    ],
    "types": [
      "@cloudflare/workers-types"
    ],
    "allowImportingTsExtensions": true,
  },
  "exclude": [
    "node_modules"
  ]
}
